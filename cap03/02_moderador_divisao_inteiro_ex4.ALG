Algoritmo "Moderador de Divisão Inteira ' \ '"


Var
   a, b, resultado : inteiro
   a2, b2, resultado2 : real

Inicio
    a <- 5
    b <- 2
    
    resultado <- a \ b
    
    escreval(resultado)
    
    a2 <- 5
    b2 <- 2
    
    resultado2 <- a / b
    
    escreval(resultado2)


Fimalgoritmo