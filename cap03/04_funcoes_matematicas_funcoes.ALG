Algoritmo "Funções Matemáticas"


Var

   x, y, z, a, b, c, d : real
   

Inicio

   x <- 2.0
   y <- 3.0
   z <- -7.0
   
   a <- Exp(x, y)  // x elevado a y potência
   b <- RaizQ(y)   // Raiz quadrada de y
   c <- Pi         // Número Pi
   d <- Abs(z)     // Número absoluto de z
   
   escreval(a)
   escreval(b)
   escreval(c)
   escreval(d)
   


Fimalgoritmo